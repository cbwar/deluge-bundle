{
  "name": "cbwar/deluge-bundle",
  "type": "library",
  "autoload": {
    "psr-4": {
      "Cbwar\\Bundle\\Deluge\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Raphael Lisch",
      "email": "cbwar@protonmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "symfony/dependency-injection": "^5.4 | ^6.0",
    "symfony/config": "^5.4 | ^6.0",
    "symfony/http-kernel": "^5.4 | ^6.0",
    "symfony/yaml": "^5.4 | ^6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "symfony/framework-bundle": "^5.4 | ^6.0"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
